{
  "name": "tms-oversikt-mikrofrontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install && chmod +x .husky/scripts/check-no-a11y-ignored.sh ",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "lint:staged": "eslint -c .eslintrc.pre-commit.js --fix --ext .ts,.tsx,.js,.jsx ."
  },
  "dependencies": {
    "@nanostores/react": "0.7.1",
    "@navikt/ds-icons": "3.1.3",
    "@navikt/aksel-icons": "5.2.1",
    "@navikt/ds-react": "5.0.0",
    "amplitude-js": "8.21.8",
    "dayjs": "1.11.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "2.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "@types/amplitude-js": "^8.16.2",
    "@vitejs/plugin-react": "4.0.4",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "7.0.0",
    "jsdom": "19.0.0",
    "lint-staged": "13.2.1",
    "msw": "1.2.2",
    "prettier": "2.8.7",
    "rollup-plugin-import-map": "2.2.2",
    "rollup-plugin-terser": "7.0.2",
    "typescript": "4.7.2",
    "vite": "4.4.9",
    "vite-plugin-css-injected-by-js": "3.1.0",
    "vitest": "0.34.3",
    "vitest-axe": "0.1.0"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc.pre-commit.js --fix",
      "prettier --cache --write"
    ],
    "*.{css,md,html,json}": [
      "prettier --cache --write"
    ]
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
