{
  "name": "tms-oversikt-mikrofrontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install && chmod +x .husky/scripts/check-no-a11y-ignored.sh ",
    "lint": "eslint --fix --ext .js,.jsx .",
    "lint:staged": "eslint -c .eslintrc.pre-commit.js --fix --ext .js,.jsx ."
  },
  "dependencies": {
    "@navikt/ds-react": "2.9.3",
    "amplitude-js": "8.21.8",
    "dayjs": "1.11.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "2.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@vitejs/plugin-react": "3.1.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "7.0.0",
    "jsdom": "19.0.0",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "rollup-plugin-import-map": "2.2.2",
    "rollup-plugin-terser": "7.0.2",
    "vite": "4.2.2",
    "vite-plugin-css-injected-by-js": "3.1.0",
    "vite-plugin-mock": "3.0.0",
    "vitest": "0.30.1"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint -c .eslintrc.pre-commit.js --fix",
      "prettier --cache --write"
    ],
    "*.{css,md,html,json}": [
      "prettier --cache --write"
    ]
  }
}
